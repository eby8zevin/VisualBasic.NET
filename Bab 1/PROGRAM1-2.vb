'Menggunakan namespace System
Imports System

'Membuat modul
Module Program
  'Membuat metode (prosedur) utama
  Sub Main()
    'Menampilkan bilangan 0 sampai 9 ke layar
    For i As Integer = 0 To 9
      Console.WriteLine(i)
    Next
  End Sub
End Module
