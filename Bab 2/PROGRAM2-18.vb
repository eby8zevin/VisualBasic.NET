Module Program
  Sub Main()        
    'mendeklarasikan variabel bertipe Object
    Dim ob As Object
        
    'memasukkan nilai Integer ke ob
    ob = 100
    Console.WriteLine("ob: {0}", ob)
    
    'memasukkan nilai Double ke ob
    ob = 12.75
    Console.WriteLine("ob: {0}", ob)
    
    'memasukkan nilai Boolean ke ob
    ob = True
    Console.WriteLine("ob: {0}", ob)
    
    'memasukkan nilai String ke ob
    ob = "Visual Basic .NET"
    Console.WriteLine("ob: {0}", ob)    
    
    Console.ReadLine()
  End Sub
End Module
